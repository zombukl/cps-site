{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [ 
    "eslint:recommended",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", { "endOfLine":"auto" }]
  }
}
